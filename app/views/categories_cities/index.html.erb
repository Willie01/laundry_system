<p id="notice"><%= notice %></p>

<h1>Categories Cities</h1>

<table>
  <thead>
    <tr>
      <th>Category</th>
      <th>City</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categories_cities.each do |categories_city| %>
      <tr>
        <td><%= categories_city.category_id %></td>
        <td><%= categories_city.city_id %></td>
        <td><%= categories_city.status %></td>
        <td><%= link_to 'Show', categories_city %></td>
        <td><%= link_to 'Edit', edit_categories_city_path(categories_city) %></td>
        <td><%= link_to 'Destroy', categories_city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Categories City', new_categories_city_path %>
