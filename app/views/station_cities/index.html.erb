<p id="notice"><%= notice %></p>

<h1>Station Cities</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @station_cities.each do |station_city| %>
      <tr>
        <td><%= station_city.city_id %></td>
        <td><%= station_city.longitude %></td>
        <td><%= station_city.latitude %></td>
        <td><%= station_city.status %></td>
        <td><%= link_to 'Show', station_city %></td>
        <td><%= link_to 'Edit', edit_station_city_path(station_city) %></td>
        <td><%= link_to 'Destroy', station_city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Station City', new_station_city_path %>
