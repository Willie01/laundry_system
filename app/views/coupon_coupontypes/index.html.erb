<p id="notice"><%= notice %></p>

<h1>Coupon Coupontypes</h1>

<table>
  <thead>
    <tr>
      <th>Coupon</th>
      <th>Coupon type</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coupon_coupontypes.each do |coupon_coupontype| %>
      <tr>
        <td><%= coupon_coupontype.coupon_id %></td>
        <td><%= coupon_coupontype.coupon_type_id %></td>
        <td><%= coupon_coupontype.status %></td>
        <td><%= link_to 'Show', coupon_coupontype %></td>
        <td><%= link_to 'Edit', edit_coupon_coupontype_path(coupon_coupontype) %></td>
        <td><%= link_to 'Destroy', coupon_coupontype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coupon Coupontype', new_coupon_coupontype_path %>
