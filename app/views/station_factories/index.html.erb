<p id="notice"><%= notice %></p>

<h1>Station Factories</h1>

<table>
  <thead>
    <tr>
      <th>Station</th>
      <th>Factory</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @station_factories.each do |station_factory| %>
      <tr>
        <td><%= station_factory.station_id %></td>
        <td><%= station_factory.factory_id %></td>
        <td><%= station_factory.status %></td>
        <td><%= link_to 'Show', station_factory %></td>
        <td><%= link_to 'Edit', edit_station_factory_path(station_factory) %></td>
        <td><%= link_to 'Destroy', station_factory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Station Factory', new_station_factory_path %>
