<p id="notice"><%= notice %></p>

<h1>Income Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Income status name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @income_statuses.each do |income_status| %>
      <tr>
        <td><%= income_status.income_status_name %></td>
        <td><%= link_to 'Show', income_status %></td>
        <td><%= link_to 'Edit', edit_income_status_path(income_status) %></td>
        <td><%= link_to 'Destroy', income_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Income Status', new_income_status_path %>
