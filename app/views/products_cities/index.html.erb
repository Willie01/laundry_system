<p id="notice"><%= notice %></p>

<h1>Products Cities</h1>

<table>
  <thead>
    <tr>
      <th>Categories city</th>
      <th>Product</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products_cities.each do |products_city| %>
      <tr>
        <td><%= products_city.categories_city_id %></td>
        <td><%= products_city.product_id %></td>
        <td><%= products_city.status %></td>
        <td><%= link_to 'Show', products_city %></td>
        <td><%= link_to 'Edit', edit_products_city_path(products_city) %></td>
        <td><%= link_to 'Destroy', products_city, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Products City', new_products_city_path %>
